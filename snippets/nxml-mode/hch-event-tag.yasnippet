# -*- mode: snippet; require-final-newline: nil -*-
# name: hch event tag
# key: ht
# binding: direct-keybinding
# --
<event>
  <image href="file:///images/${4:$(downcase (replace-regexp-in-string ".+ " "" yas/text))}.jpg"/>
  <date>$1.$2.18</date>
  <name>$4</name>
  <bio>$5</bio>
  <title>$6</title>
</event>
$0